{
  "name": "tech-blog",
  "version": "0.0.1",
  "author": "barbajoe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Barbacoa08/tech-blog/"
  },
  "keywords": [
    "pnpm",
    "css",
    "svelte",
    "sveltekit`",
    "blog"
  ],
  "type": "module",
  "scripts": {
    "all": "pnpm clean && bun install && pnpm format && pnpm lint && pnpm check && pnpm build && pnpm test",
    "clean": "rm -rf .netlify .svelte-kit build",
    "start": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-netlify": "3.0.0",
    "@sveltejs/kit": "2.0.0",
    "@sveltejs/vite-plugin-svelte": "3.0.1",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.35.1",
    "prettier": "3.1.1",
    "prettier-plugin-svelte": "3.1.2",
    "svelte": "5.0.0-next.1",
    "svelte-check": "3.6.2",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vitest": "1.0.4"
  }
}
