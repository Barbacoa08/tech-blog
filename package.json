{
  "name": "tech-blog",
  "version": "1.0.0",
  "author": "barbajoe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Barbacoa08/tech-blog/"
  },
  "keywords": [
    "pnpm",
    "css",
    "svelte",
    "sveltekit`",
    "blog"
  ],
  "type": "module",
  "scripts": {
    "all": "pnpm clean && pnpm install && pnpm format && pnpm lint && pnpm check && pnpm build && pnpm test",
    "github": "pnpm i && pnpm lint && pnpm check && pnpm build && pnpm test",
    "clean": "rm -rf .netlify .svelte-kit build",
    "start": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write . --log-level warn"
  },
  "devDependencies": {
    "@barbajoe/css-lib": "0.1.8",
    "@barbajoe/svelte-lib": "0.2.12",
    "@sveltejs/adapter-netlify": "3.0.1",
    "@sveltejs/kit": "2.0.6",
    "@sveltejs/vite-plugin-svelte": "3.0.1",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "@zerodevx/svelte-toast": "0.9.5",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.35.1",
    "prettier": "3.1.1",
    "prettier-plugin-svelte": "3.1.2",
    "svelte": "4.2.8",
    "svelte-check": "3.6.2",
    "svelte-search": "2.0.2",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vitest": "1.1.0"
  }
}
